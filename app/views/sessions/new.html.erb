<h1 align=center>Sign In</h1>
<%= form_tag sessions_path do %>
	<table align=center>
	<tr>
		<td><%= label_tag :name %>
		<td><%= text_field_tag :name, params[:name] %>
	</tr>
	<tr>
		<td><%= label_tag :password %>
		<td><%= password_field_tag :password %>
	</tr>
	<tr>
		<td><%= submit_tag "Sign in" %></td>
	</tr>
	</table>
<%end%>

